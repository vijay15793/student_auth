<html >
  <head>
    <title>Students | <%= @page_title || 'Home' %></title>
    
    <%= stylesheet_link_tag('application', :media => 'all') %>
    <%= javascript_include_tag('application') %>
  </head>
  <body>
    <div id="header">
      <h1><%= link_to(image_tag("home.jpg", :size =>'50x50'), {:action =>'index'}) %> &nbsp;&nbsp; Students:Data</h1>
    </div>
    <div id="main">
      <% if !flash[:notice].blank? %>
      <div class="notice">
        <%= flash[:notice] %>
      </div>
      <% end %>
      <div id="content">
        <%= yield %>
      </div>
    </div>
    <div id="footer">
      <p id="copyright">&copy; T-chowk labs/Vijay Singh</p>
    </div>
  </body>
</html>